    "drizzle-orm": "^0.40.0",
    "drizzle-seed": "^0.3.1",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "firebase-admin": "^13.2.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.13.3",
